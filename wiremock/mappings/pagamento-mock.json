{
  "request": {
    "method": "POST",
    "url": "/pagamentos/enviar",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.amount)]"
      },
      {
        "matchesJsonPath": "$[?(@.payment.type)]"
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "pagamento-resposta.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
